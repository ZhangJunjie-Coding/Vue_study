.$emit('zhang',this.name,1,24,4)\r\n            this.$emit('demo')\r\n        },\r\n        unbind(){\r\n            // this.$off('zhang')      //解绑一个自定义事件\r\n            // this.$off(['zhang','demo']) //解绑多个自定义事件\r\n            this.$off() //解绑所有自定义事件\r\n        },\r\n        death(){\r\n            this.$destroy() //销毁了当前Student组件的实例,销毁后\r\n        },\r\n        add(){\r\n            this.number++;\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n.student{\r\n    background-color: orange;\r\n    padding: 5px;\r\n    margin-top: 30px;\r\n   \r\n}\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}